<!DOCTYPE html>
<html>

<head>
  <title>Random Movies</title>
  <style>
    li {
      padding-bottom: 20px;
    }
  </style>
</head>

<body>
  <h1>Here are 10 random movies:</h1>
  <ul>
    <% movies.forEach(movie=> { %>
      <li>
        <strong>
          <%= movie.Title %>
        </strong><br />
        <img src="<%= movie.Poster %>" alt="<%= movie.Title %>" width="150" />
      </li>
      <% }) %>
  </ul>
</body>

</html>